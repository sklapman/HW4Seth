<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HighestImportance">

    <TextView
        android:id="@+id/textViewHighBirdName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:text="Highest Bird Name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewHighBirdSpotter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="65dp"
        android:text="Bird Spotter"
        app:layout_constraintEnd_toEndOf="@+id/textViewHighBirdName"
        app:layout_constraintStart_toStartOf="@+id/textViewHighBirdName"
        app:layout_constraintTop_toBottomOf="@+id/textViewHighBirdName" />

    <TextView
        android:id="@+id/textViewHighZip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="55dp"
        android:text="Zip"
        app:layout_constraintEnd_toEndOf="@+id/textViewHighBirdName"
        app:layout_constraintStart_toStartOf="@+id/textViewHighBirdName"
        app:layout_constraintTop_toBottomOf="@+id/textViewHighBirdSpotter" />

</androidx.constraintlayout.widget.ConstraintLayout>